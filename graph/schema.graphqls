
scalar Time

type UserMetaData{
    id: ID!
    phoneNumber: String!
    dateOfBirth: Time!
    createdDate: Time!
}


input CreateUserData{ 
    phoneNumber: String!
    dateOfBirth: Time!
}

type Mutation{
     createUser(data: CreateUserData!): UserMetaData!
}

type Query{
    userMetaData(id: ID!): UserMetaData!
}
